.col-lg-3.col-12.float-left.pr-lg-3
  .Box.border-0
    .Box-body
      Hi
    .Box-footer
      HiHI

.col-lg-9.col-12.float-left
  - @tracks_by_date.each do |date, tracks|
    .Box.mb-2.track
      .Box-header
        %h3.Box-title
          = date
      - tracks.group_by(&:owner_id).each do |owner_id, ts|
        .Box-body
          =owner_id
          %ul
            =render ts
  =render 'layouts/pagination', pagy: @pagy
